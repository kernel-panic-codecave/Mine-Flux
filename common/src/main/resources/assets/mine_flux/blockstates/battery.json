        "west": "output"
      },
      "apply": {
        "model": "mine_flux:block/battery_output",
        "y": 270
      }
    },
    {
      "when": {
        "up": "output"
      },
      "apply": {
        "model": "mine_flux:block/battery_output",
        "x": 270
      }
    },
    {
      "when": {
        "down": "output"
      },
      "apply": {
        "model": "mine_flux:block/battery_output",
        "x": 90
      }
