{
  "multipart": [
    {
      "apply": {
        "model": "mine_flux:block/battery_frame"
      }
    },
    {
      "when": {
        "north": "input"
      },
      "apply": {
        "model": "mine_flux:block/battery_input"
      }
    },
    {
      "when": {
        "east": "input"
      },
      "apply": {
        "model": "mine_flux:block/battery_input",
        "y": 90
      }
    },
    {
      "when": {
        "south": "input"
      },
      "apply": {
        "model": "mine_flux:block/battery_input",
        "y": 180
      }
    },
    {
      "when": {
        "west": "input"
      },
      "apply": {
        "model": "mine_flux:block/battery_input",
        "y": 270
      }
    },
    {
      "when": {
        "up": "input"
      },
      "apply": {
        "model": "mine_flux:block/battery_input",
        "x": 270
      }
    },
    {
